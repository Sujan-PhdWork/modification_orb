{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug ORB-SLAM2",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/Examples/RGB-D/rgbd_tum",
        "args": [
          "${workspaceFolder}/Vocabulary/ORBvoc.txt",
          "${workspaceFolder}/Examples/RGB-D/TUM1.yaml",
          "${workspaceFolder}/../dataset/rgbd_dataset_freiburg1_xyz/",
          "${workspaceFolder}/Examples/RGB-D/associations/fr1_xyz.txt"
        ],
        "stopAtEntry": true,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "miDebuggerPath": "/usr/bin/gdb",
        "logging": {
            "engineLogging": true
        },
        
      }
    ]
  }
  